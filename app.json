{
  "expo": {
    "name": "Betterspace",
    "slug": "Betterspace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splashIcon.png",
    "scheme": "betterspace",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splashIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4e7560"
    },
    "reactNativeGestureHandler": {
      "touchableOpacityActiveOpacity": 1
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/splashIcon.png",
        "resizeMode": "contain",
        "backgroundColor": "#4e7560"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splashIcon.png",
        "backgroundColor": "#4e7560"
      },
      "splash": {
        "image": "./assets/images/splashIcon.png",
        "resizeMode": "contain",
        "backgroundColor": "#4e7560"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-router"]
  }
}
