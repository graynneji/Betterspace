{
  "expo": {
    "name": "Betterspace",
    "slug": "Betterspace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "betterspace",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splashIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4e7560"
    },
    "reactNativeGestureHandler": {
      "touchableOpacityActiveOpacity": 1
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/splashIcon.png",
        "resizeMode": "contain",
        "backgroundColor": "#4e7560"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#4e7560"
      },
      "splash": {
        "image": "./assets/images/splashIcon.png",
        "resizeMode": "contain",
        "backgroundColor": "#4e7560"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.graynneji.Betterspace"
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bbc96568-93ec-46a0-95b6-c8380b54bf25"
      }
    }
  }
}
