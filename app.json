{
  "expo": {
    "name": "Betterspace",
    "slug": "Betterspace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "betterspace",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splashIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4e7560"
    },
    "reactNativeGestureHandler": {
      "touchableOpacityActiveOpacity": 1
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/splashIcon.png",
        "resizeMode": "contain",
        "backgroundColor": "#4e7560"
      },
      "infoPlist": {
        "NSCalendarsUsageDescription": "Betterspace needs calendar access to save your appointments.",
        "NSRemindersUsageDescription": "Betterspace needs reminders access to manage appointment alerts."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#4e7560"
      },
      "permissions": ["READ_CALENDAR", "WRITE_CALENDAR"],
      "splash": {
        "image": "./assets/images/splashIcon.png",
        "resizeMode": "contain",
        "backgroundColor": "#4e7560"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.graynneji.Betterspace"
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos."
        }
      ],

      [
        "expo-image-picker",
        {
          "photosPermission": "Betterspace accesses your photos for you to post on community and set profile pictures."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bbc96568-93ec-46a0-95b6-c8380b54bf25"
      }
    }
  }
}
